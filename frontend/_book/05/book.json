{
    "title": "我的个人博客",
    "gitbook": "3.2.2",
    "language": "zh-hans",
    "plugins": [
        "anchor-navigation-ex",
        "emphasize",
        "splitter",
        "expandable-chapters-small",
        "multipart",
        "prism",
        "-highlight",
        "-sharing"
    ],
    "pluginsConfig": {
        "prism": {
            "css": [
              "prismjs/themes/prism-okaidia.css"
            ]
          },
        "anchor-navigation-ex": {
                "showLevel": true,
                "associatedWithSummary": true,
                "mode": "float",
                "float": {
                    "showLevelIcon": false,
                    "level1Icon": "fa fa-hand-o-right",
                    "level2Icon": "fa fa-hand-o-right",
                    "level3Icon": "fa fa-hand-o-right"
                },
                "pageTop": {
                    "showLevelIcon": false,
                    "level1Icon": "fa fa-hand-o-right",
                    "level2Icon": "fa fa-hand-o-right",
                    "level3Icon": "fa fa-hand-o-right"
                }
        }
    }
}